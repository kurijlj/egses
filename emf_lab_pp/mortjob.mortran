write(56,'(1X,1P,5E14.5)')DELCMP,(ALPHIP(I),BPARP(I),
   DELPOSP(I),I=1,2);
write(56,'(1X,1P,5E14.5)')XR0P,TEFF0P,BLCCP,XCCP;
write(56,'(1X,1P,5E14.5)')BXE,AXE;
