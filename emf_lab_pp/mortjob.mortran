    IF( rng_seed > $NRANDOM ) [
        rng_seed = $NRANDOM;
        call egs_fill_rndm_array(rng_seed,rng_array);
        rng_seed = 1;
    ]
    {P1} = rng_array(rng_seed); rng_seed = rng_seed + 1;
};
